    "flex-layout.row#3-header-desktop": {
        "props": {
            "blockClass": ["main-header","menu-header-desk"],
            "horizontalAlign": "left",
            "verticalAlign": "center",
            "preventHorizontalStretch": true,
            "preventVerticalStretch": true,
            "fullWidth": true
        },
        "children": [
            "menu-item#all-departments",
            "vtex.menu@2.x:menu#main-menu-header-desktop"
        ]
    },
    "vtex.menu@2.x:menu#main-menu-header-desktop": {
        "props": {
            "blockClass": [
                "main-menu-header-desktop"
            ]
        },
        "children": [
            "menu-item#header-department1",
            "menu-item#header-department2",
            "menu-item#news",
            "menu-item#outlet"
        ]
    },
    "menu-item#all-departments": {
        "props": {
            "blockClass": [],
            "id": "menu-item-all-departments",
            "itemProps": {
                "tagTitle": "all-departments",
                "text": "TODOS OS DEPARTAMENTOS"
            }
        },
        "blocks": [
            "vtex.menu@2.x:submenu#all-departments"
        ]
    },
