            "itemProps": {
                "tagTitle": "partner-stores",
                "text": "LOJAS PARCEIRAS"
            }
        }
    },
    ///// -------------------------------------- acordion category 
    "vtex.menu@2.x:submenu.accordion#department-accordion": {
        "children": [
            "vtex.menu@2.x:menu#department-accordion-itens"
        ]
    },
    "vtex.menu@2.x:menu#department-accordion-itens": {
        "props": {

            "blockClass": ["full-width"],
            "orientation": "vertical"
                       
        },
        "children": [
            "menu-item#category1-accordion",
            "menu-item#category2-accordion",
            "menu-item#category3-accordion",
            "menu-item#see-more-accordion-category"
        ]
    },   
    "menu-item#category1-accordion": {
        "props": {           
            "blockClass": ["drawer-category"],
            "id": "menu-item-category1-accordion",
            "itemProps": {
                "tagTitle": "category1",
                "text": "CATEGORIA 1"
            }
        },
        "blocks": [
            "vtex.menu@2.x:submenu.accordion#category-accordion"
        ]
    },
    "menu-item#category2-accordion": {
        "props": {            
            "blockClass": ["drawer-category"],
            "id": "menu-item-category2-accordion",
            "itemProps": {
                "tagTitle": "category2",
                "text": "CATEGORIA 2"
            }
        },
        "blocks": [
            "vtex.menu@2.x:submenu.accordion#category-accordion"
        ]
    },
    "menu-item#category3-accordion": {
        "props": {
            "blockClass": ["drawer-category"],
            "id": "menu-item-category3-accordion",
            "itemProps": {
                "tagTitle": "category3",
                "text": "CATEGORIA 3"
            }
        },
        "blocks": [
            "vtex.menu@2.x:submenu.accordion#category-accordion"
        ]
    },    
    "menu-item#see-more-accordion-category": {
        "props": {
            "blockClass": ["drawer-see-more","drawer-category"],
            "id": "menu-item-see-more-category",
            "itemProps": {
                "tagTitle": "see-more-category",
                "text": "Ver Todos"
            }
        }
    },


    ///// -------------------------------------- subcategory accordion ( can be props )
    "vtex.menu@2.x:submenu.accordion#category-accordion": {
        "children": [
            "vtex.menu@2.x:menu#category-itens-accordion"
        ]
    },
    "vtex.menu@2.x:menu#category-itens-accordion": {
        "props": {
            "orientation": "vertical"            
        },
        "children": [
            "menu-item#subcategory1",
            "menu-item#subcategory2",
            "menu-item#subcategory3",
            "menu-item#see-more-accordion"
        ]
    },
    "menu-item#subcategory1": {
        "props": {           
            "blockClass": ["drawer-subcategory"],
            "id": "menu-item-subcategory1",
            "itemProps": {
                "tagTitle": "subcategory1",
                "text": "Subcategoria 1"
            }
        }
    },
    "menu-item#subcategory2": {
        "props": {           
            "blockClass": ["drawer-subcategory"],
            "id": "menu-item-subcategory2",
            "itemProps": {
                "tagTitle": "subcategory2",
                "text": "Subcategoria 2"
            }
        }
    },
    "menu-item#subcategory3": {
        "props": {           
            "blockClass": ["drawer-subcategory"],
            "id": "menu-item-subcategory3",
            "itemProps": {
                "tagTitle": "subcategory3",
                "text": "Subcategoria 3"
            }
        }
    },
    "menu-item#see-more-accordion": {
        "props": {           
            "blockClass": ["drawer-see-more","drawer-subcategory"],
            "id": "menu-item-see-more-accordion",
            "itemProps": {
                "tagTitle": "see-more3",
                "text": "Ver todos"
            }
        }
    }

}