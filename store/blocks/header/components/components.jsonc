            "itemProps": {
                "tagTitle": "novidades",
                "text": "NOVIDADES"
            }
        }
    },
    "menu-item#outlet": {
        "props": {
            "blockClass": ["blue-bold","drawer-margin"],
            "id": "menu-item-outlet",
            "itemProps": {
                "tagTitle": "outlet",
                "text": "OUTLET"
            }
        }
    },
    "menu-item#department4": {
        "props": {
            "blockClass": ["drawer-margin"],
            "id": "menu-item-department4",
            "itemProps": {
                "tagTitle": "department4",
                "text": "DEPARTAMENTO 4"
            }
        }
    },
    "menu-item#my-account": {
        "props": {
            "blockClass": ["my-account"],
            "id": "menu-item-my-account",
            "itemProps": {
                "tagTitle": "my-account",
                "text": "MINHA CONTA"
            }
        }
    },
    "menu-item#partner-stores": {
        "props": {
            "blockClass": ["drawer-partner"],
