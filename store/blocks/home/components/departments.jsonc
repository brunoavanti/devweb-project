{
    "flex-layout.row#home-departments": {
        "props": {
            "blockClass": "home-department-row"
        },
        "children": [
            "flex-layout.col#home-departments"
        ]
    },
    "flex-layout.col#home-departments": {
        "children": [
            "rich-text#home-department-title",
            "slider-layout#home-departments"
        ]
    },
    "rich-text#home-department-title": {
        "props": {
            "textAlignment": "CENTER",
            "textPosition": "CENTER",
            "text": "**TODOS OS DEPARTAMENTOS**",
            "blockClass": "home-department-title"
        }
    },
    "slider-layout#home-departments": {
        "props": {
            "itemsPerPage": {
                "desktop": 8,
                "tablet": 3,
                "phone": 3
            },
            "infinite": true,
            "showNavigationArrows": "always",
            "showPaginationDots": "never",
            "blockClass": "carousel-departments"
        },
        "children": [
            "flex-layout.col#slider-department-1",
            "flex-layout.col#slider-department-2",
            "flex-layout.col#slider-department-3",
            "flex-layout.col#slider-department-4",
            "flex-layout.col#slider-department-5",
            "flex-layout.col#slider-department-6",
            "flex-layout.col#slider-department-7",
            "flex-layout.col#slider-department-8",
